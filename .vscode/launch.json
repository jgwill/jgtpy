  "version": "0.2.0",
  "configurations": [

    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "jgtcli-ads",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/jgtpy/jgtcli.py",
      "args": [
        "-i","USD/CAD","-t","m5","--ads","-v","2"
      ],
      "console": "internalConsole",
      "justMyCode": false
    },
    {
      "name": "ADS-tst-chart",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/test-ads.py",
      "args": [
        "SPX500"
      ],
      "console": "internalConsole",
      "justMyCode": false
    }
  ]
}